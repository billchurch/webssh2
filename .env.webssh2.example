# WebSSH2 Environment Variables
# Copy this file to .env.webssh2 and customize for your environment

# Server Configuration
WEBSSH2_LISTEN_IP=0.0.0.0
WEBSSH2_LISTEN_PORT=2222

# Default SSH Connection Settings
WEBSSH2_SSH_HOST=
WEBSSH2_SSH_PORT=22
WEBSSH2_SSH_TERM=xterm-256color

# SSH Security - Choose one approach:

# Option 1: Use a preset (recommended)
WEBSSH2_SSH_ALGORITHMS_PRESET=modern
# Available presets: modern, legacy, strict

# Option 2: Custom algorithm configuration (comment out preset above)
# WEBSSH2_SSH_ALGORITHMS_CIPHER=aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr
# WEBSSH2_SSH_ALGORITHMS_KEX=ecdh-sha2-nistp256,ecdh-sha2-nistp384,ecdh-sha2-nistp521
# WEBSSH2_SSH_ALGORITHMS_HMAC=hmac-sha2-256,hmac-sha2-512
# WEBSSH2_SSH_ALGORITHMS_COMPRESS=none,zlib@openssh.com
# WEBSSH2_SSH_ALGORITHMS_SERVER_HOST_KEY=ecdsa-sha2-nistp256,ecdsa-sha2-nistp384,ssh-rsa

# SSH Connection Options
WEBSSH2_SSH_READY_TIMEOUT=20000
WEBSSH2_SSH_KEEPALIVE_INTERVAL=120000
WEBSSH2_SSH_KEEPALIVE_COUNT_MAX=10
WEBSSH2_SSH_ALWAYS_SEND_KEYBOARD_INTERACTIVE=false
WEBSSH2_SSH_DISABLE_INTERACTIVE_AUTH=false

# Web Interface Customization
WEBSSH2_HEADER_TEXT=SSH Gateway
WEBSSH2_HEADER_BACKGROUND=green

# CORS Configuration
# Development (permissive)
WEBSSH2_HTTP_ORIGINS=http://localhost:*,https://localhost:*,http://127.0.0.1:*
# Production (restrictive)
# WEBSSH2_HTTP_ORIGINS=https://yourdomain.com,https://admin.yourdomain.com

# Default User Credentials (optional - can be overridden per connection)
WEBSSH2_USER_NAME=
WEBSSH2_USER_PASSWORD=
WEBSSH2_USER_PRIVATE_KEY=
WEBSSH2_USER_PASSPHRASE=

# Session Management
WEBSSH2_SESSION_SECRET=change-this-in-production
WEBSSH2_SESSION_NAME=webssh2.sid

# UI Options
WEBSSH2_OPTIONS_CHALLENGE_BUTTON=true
WEBSSH2_OPTIONS_AUTO_LOG=false
WEBSSH2_OPTIONS_ALLOW_REAUTH=true
WEBSSH2_OPTIONS_ALLOW_RECONNECT=true
WEBSSH2_OPTIONS_ALLOW_REPLAY=true

# Network Restrictions (optional)
WEBSSH2_SSH_ALLOWED_SUBNETS=

# Debug Output (optional - for troubleshooting)
# DEBUG=webssh2:*
# DEBUG=webssh2:config,webssh2:envConfig

# Legacy support (use WEBSSH2_LISTEN_PORT instead)
# PORT=2222