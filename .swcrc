{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": true
  },
  "module": {
    "type": "nodenext"
  },
  "sourceMaps": true,
  "exclude": [
    "node_modules",
    "dist",
    "coverage",
    "client",
    "public"
  ]
}